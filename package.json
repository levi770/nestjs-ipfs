{
  "name": "@mfsoftworks/nestjs-ipfs",
  "version": "1.0.2",
  "description": "A module for utilizing IPFS p2p filesystem with NestJS",
  "author": "MF Softworks <arranjamesfletcher2012@gmail.com>",
  "license": "MIT",
  "url": "https://github.com/mfsoftworks/nestjs-ipfs#readme",
  "funding": {
    "type": "paypal",
    "url": "https://www.paypal.me/nygmarose"
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.json",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint 'lib/**/*.ts' --fix",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "test:e2e": "jest --config ./tests/jest-e2e.json --runInBand",
    "test:e2e:dev": "jest --config ./tests/jest-e2e.json --runInBand --watch",
    "prerelease": "npm run build",
    "release": "env-cmd release-it"
  },
  "keywords": [
    "nestjs",
    "ipfs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mfsoftworks/nestjs-ipfs"
  },
  "peerDependencies": {
    "@nestjs/common": "^9.2.0",
    "@nestjs/core": "^9.2.0",
    "ipfs": "^0.65.0"
  },
  "dependencies": {
    "ipfs": "^0.51.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.5",
    "@nestjs/common": "^9.2.0",
    "@nestjs/core": "^9.2.0",
    "@nestjs/platform-express": "^9.2.0",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.0",
    "@types/express": "^4.17.14",
    "@types/jest": "29.2.2",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^29.2.2",
    "prettier": "^2.7.1",
    "reflect-metadata": "^0.1.13",
    "release-it": "^15.5.0",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.7",
    "supertest": "^6.3.1",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  }
}
